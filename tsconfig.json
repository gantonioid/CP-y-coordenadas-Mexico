{
    // "extends": "../../tsconfig",
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": false,
        "jsx": "react",
        "allowJs": false,
        "lib": [
            "es2016",
            "dom"
        ],
        "declaration": true,
        "sourceMap": true,
        "moduleResolution": "node",
        "baseUrl": "./",
        "resolveJsonModule": true,
        "target": "es6",
        "module": "commonjs",
        "allowSyntheticDefaultImports": true
    },
    "include": [
        "./src/**/*"
    ]
}